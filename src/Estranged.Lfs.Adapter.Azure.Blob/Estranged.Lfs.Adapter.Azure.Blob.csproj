<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>3.1.0</Version>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>alanedwardes,stephanvs</Authors>
    <Description>An Azure Storage Accounts adapter for Estranged.Lfs</Description>
    <PackageId>Estranged.Lfs.Adapter.Azure.Blob</PackageId>
    <PackageProjectUrl>https://github.com/alanedwardes/Estranged.Lfs</PackageProjectUrl>
    <PackageTags>git lfs azure blob</PackageTags>
    <AssemblyName>Estranged.Lfs.Adapter.Azure.Blob</AssemblyName>
    <RootNamespace>Estranged.Lfs.Adapter.Azure.Blob</RootNamespace>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Storage.Blobs" Version="12.13.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Estranged.Lfs.Data\Estranged.Lfs.Data.csproj" />
  </ItemGroup>

</Project>
